{
  "version": 1,
  "paths": {
    "/tasks": {
      "name": "/tasks",
      "lambdaFunction": "modifyTask",
      "permissions": {
        "setting": "open"
      }
    },
    "/profile/{userId}": {
      "name": "/profile/{userId}",
      "lambdaFunction": "userProfileHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/lists/{listId}": {
      "name": "/lists/{listId}",
      "lambdaFunction": "tasksHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/listsMetadata": {
      "name": "/listsMetadata",
      "lambdaFunction": "todoListHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/collaborators": {
      "name": "/collaborators",
      "lambdaFunction": "collaboratorsHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}